<!--  START section#contact  -->
<section id="contact" class="container-fluid">
  <div class="row">
    <!--  SOURCE https://codepen.io/jaycbrf/pen/iBszr  -->
    <!--  START form#contact-form  -->
    <form class="form-horizontal col-xs-12 col-md-8 col-lg-6 hidden-print" id="contact-form" method="post" action="/cgi/forms.php">
      <fieldset class="animate slide-right">
        <legend class="sr-only">Contact form</legend>
        <h2 class="col-xs-12">Contact us today!</h2>
        <div class="form-group">
          <label class="col-md-4 control-label" for="contact-form-name-input">Full Name&ensp;<span class="fa fa-asterisk form-required"><small class="sr-only">required</small></span></label>
          <div class="col-md-8 inputGroupContainer">
            <div class="input-group">
              <span class="input-group-addon"><span class="fa fa-user"></span></span>
              <input id="contact-form-name-input" name="full_name" placeholder="Full name" class="form-control" type="text" required>
            </div>
            <small id="name_error" class="form-error text-muted"></small>
          </div>
        </div>
        <div class="form-group">
          <label class="col-md-4 control-label" for="contact-form-email-input">E-Mail&ensp;<span class="fa fa-asterisk form-required"><small class="sr-only">required</small></span></label>
          <div class="col-md-8 inputGroupContainer">
            <div class="input-group">
              <span class="input-group-addon"><span class="fa fa-envelope"></span></span>
              <input id="contact-form-email-input" name="email" placeholder="E-Mail address" class="form-control" type="text" required>
            </div>
            <small id="email_error" class="form-error text-muted"></small>
          </div>
        </div>
        <div class="form-group">
          <label class="col-md-4 control-label" for="contact-form-phone-input">Phone Number</label>
          <div class="col-md-8 inputGroupContainer">
            <div class="input-group">
              <span class="input-group-addon"><span class="fa fa-mobile-phone"></span></span>
              <input id="contact-form-phone-input" name="phone" placeholder="(510) 555-1212" class="form-control" type="text">
            </div>
            <small id="phone_error" class="form-error text-muted"></small>
          </div>
        </div>
        <div class="form-group">
          <label class="col-md-4 control-label">May we text this number?</label>
          <div class="col-md-8 radio-wrapper">
            <span class="radio">
              <label>
                Yes <input type="radio" name="texting" value="yes" />
              </label>
            </span>
            <span class="radio">
              <label>
                No <input type="radio" name="texting" value="no" />
              </label>
            </span>
          </div>
        </div>
        <div class="form-group">
          <label class="col-md-4 control-label" for="contact-form-message">Message&ensp;<span class="fa fa-asterisk form-required"><small class="sr-only">required</small></span></label>
          <div class="col-md-8 inputGroupContainer">
            <div class="input-group">
              <span class="input-group-addon"><span class="fa fa-pencil"></span></span>
              <textarea id="contact-form-message" class="form-control" name="message" placeholder="Questions and comments" required></textarea>
            </div>
            <small id="message_error" class="form-error text-muted"></small>
          </div>
        </div>
        <div class="alert alert-success" role="alert" id="contact-success-message">
          Success <span class="fa fa-thumbs-up"></span> Thanks for contacting us. We will get back to you soon.
        </div>
        <div class="alert alert-warning" role="alert" id="contact-warning-message">
          Invalid <span class="fa fa-thumbs-down"></span> There is a problem with the form. Correct it and submit again.
        </div>
        <div class="alert alert-danger" role="alert" id="contact-failure-message">
          Failure <span class="fa fa-warning"></span> There was a technical problem submitting the form. Try again later.
        </div>
        <div class="form-group">
          <label class="col-md-4 control-label"></label>
          <div class="col-md-4">
            <button id="contact-submit" type="submit" class="btn btn-green">Send <span class="fa fa-send"></span></button>
          </div>
        </div>
      </fieldset>
    </form>
    <!--  END form#contact-form  -->
